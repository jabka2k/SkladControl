    <style>
        @import url('https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');
    </style>


    <link rel="stylesheet" type="text/css" href="/css/reports.css">
    <title></title>
</head>
<body>
<div class="wrapper">
    <header class="header">
        <div class="container">
            <div class="header_body">
                <div class="logo_block">
                    <img src="/css/logo_white.svg" alt="логотип" style="margin-right: 30px">
                    <p style="text-align: left" class="header_logo">Склад<br>Контроль</p>
                </div>
                <div class="header_burger">
                    <span></span>
                </div>
                <nav class="header_menu">
                    <ul class="header_list">
                        <li><a href="#" class="header_list">Отчеты</a></li>
                        <li><a href="#" class="header_list">Пользователи</a></li>
                        <li><a href="#"><img src="/css/Vector.svg" alt="профиль"></a></li>
                        <li><a href="/api/logout" class="header_list">Выйти</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    <div class="main">
        <div class="container">
            <h2 style="font-size: 40pt">Пользователи</h2>
            <div class="users">
                <form onsubmit="return false;" class="user">
                    <p><b>id</b></p>
                    <input name="role" type="radio" value="control" id = "control">
                    <label for = control>сотрудник отдела качества</label>
                    <input name="role" type="radio" value="sklad" id = "sklad">
                    <label for="sklad">работник склада</label>
                    <div class="input-group role_btn">
                        <button name="submit" class="btn">сохранить</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<script src="js/users.js"></script>
